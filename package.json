{
  "private": true,
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "npm run version && npm run build-es && npm run build-lib && npm run build-umd && node ./scripts/gen_less_entry.js",
    "build-lib": "cross-env BUILD_TYPE=lib pnpm run father-build",
    "build-es": "cross-env BUILD_TYPE=es pnpm run father-build",
    "build-umd": "cross-env BUILD_TYPE=umd pnpm run father-build",
    "father-build": "pnpm -r --filter=./packages/* run build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "publish": "pnpm run build && pnpm -r --filter=./packages/* publish",
    "publish:beta": "pnpm run build && pnpm -r --filter=./packages/* publish --tag beta",
    "prepublishOnly": "npm run build",
    "clean-dist": "rimraf ./packages/*/dist",
    "version": "node ./scripts/gen_version",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "antd": "^4.22.3",
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^13.0.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^18.6.4",
    "@types/react": "^18.0.16",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "babel-plugin-import": "^1.13.5",
    "chalk": "4.1.2",
    "cross-env": "^7.0.3",
    "dumi": "^1.1.0",
    "father": "^4.0.0-rc.2",
    "gh-pages": "^3.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "typescript-transform-paths": "^3.3.1",
    "yorkie": "^2.0.0"
  },
  "license": "MIT"
}
